<% provide(:title, @user.name) %>
<div class="row">
	<aside class="span4">
	    <section>
			<h1>
			<%= link_to image_tag("pizzaprof.jpg", alt: "Lorem Pizza"), 'http://rubyonrails.org/' %>
			<%= @user.name %>
			<% if @user.organization.nil? == false %>
			<h2><%=  @organization.name %></h2>
			<% else %>
			<h2>You are not in an org :(</h2>
			<% end %>
			</h1>
		</section>
	</aside>
</div>