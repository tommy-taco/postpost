<% provide(:title, "Edit Your Organization") %>
<h1>Update your Organization</h1>

<div class="row">
  <div class="span6 offset3">
    <%= form_for(@organization) do |f| %>
      <%= render 'shared/org_errors' %>

      <%= f.label :name %>
      <%= f.text_field :name %>

      <%= f.submit "Save changes", class: "btn btn-large btn-primary" %>
    <% end %>
  </div>
</div>